   <m:mrow>
    <m:mn>33</m:mn></m:mrow>
   <m:mrow>
    <m:mn>65</m:mn></m:mrow>
  </m:mfrac>
  <m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>d</m:mi><m:mo>.</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
   <m:mrow>
    <m:mn>63</m:mn></m:mrow>
   <m:mrow>
    <m:mn>65</m:mn></m:mrow>
  </m:mfrac>
