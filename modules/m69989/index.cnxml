          <m:mfrac>
           <m:mn>3</m:mn>
           <m:mn>5</m:mn>
          </m:mfrac>
          </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>12</m:mn></m:mrow>
           <m:mrow>
            <m:mn>13</m:mn></m:mrow>
          </m:mfrac>
          </m:mrow>
