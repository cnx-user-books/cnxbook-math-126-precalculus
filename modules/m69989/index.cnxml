          <item>For<m:math>
            <m:mrow>
              <m:mtext> </m:mtext><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
            </m:mrow>
          </m:math>if<m:math>
            <m:mrow>
              <m:mtext>  </m:mtext><m:mi>sin</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mo>=</m:mo><m:mfrac>
                <m:mn>3</m:mn>
                <m:mn>5</m:mn>
              </m:mfrac>
              <m:mtext> </m:mtext>
            </m:mrow>
          </m:math>and<m:math>
            <m:mrow>
              <m:mtext> </m:mtext><m:mi>cos</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mo>=</m:mo><m:mfrac>
                <m:mn>4</m:mn>
                <m:mn>5</m:mn>
              </m:mfrac>
              <m:mo>,</m:mo>
            </m:mrow>
          </m:math> then
            <equation id="fs-id1512831" class="unnumbered"><m:math display="block">
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mfrac>
       <m:mn>3</m:mn>
       <m:mn>5</m:mn>
      </m:mfrac>
      </m:mrow>
     <m:mrow>
      <m:mfrac>
       <m:mn>4</m:mn>
       <m:mn>5</m:mn>
      </m:mfrac>
      </m:mrow>
    </m:mfrac>
    <m:mo>=</m:mo><m:mfrac>
     <m:mn>3</m:mn>
     <m:mn>4</m:mn>
    </m:mfrac>
    
   </m:mtd>
  </m:mtr>
 </m:mtable>
 
</m:math>

            </equation><para id="fs-id1837927"><newline/>
If<m:math>
              <m:mrow>
                <m:mtext> </m:mtext><m:mi>sin</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
                  <m:mrow>
                    <m:mn>12</m:mn>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>13</m:mn>
                  </m:mrow>
                </m:mfrac>
                <m:mtext> </m:mtext>
              </m:mrow>
            </m:math>and<m:math>
              <m:mrow>
                <m:mtext> </m:mtext><m:mi>cos</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
                  <m:mn>5</m:mn>
                  <m:mrow>
                    <m:mn>13</m:mn>
                  </m:mrow>
                </m:mfrac>
                <m:mo>,</m:mo>
              </m:mrow>
            </m:math>
              then
<newline/></para><equation id="fs-id2917183" class="unnumbered"><m:math display="block">
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi><m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>12</m:mn></m:mrow>
       <m:mrow>
        <m:mn>13</m:mn></m:mrow>
      </m:mfrac>
      </m:mrow>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>5</m:mn></m:mrow>
       <m:mrow>
        <m:mn>13</m:mn></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mfrac>
    <m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>12</m:mn></m:mrow>
     <m:mn>5</m:mn>
    </m:mfrac>
    
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
 </m:mtable>
 
</m:math>
</equation><para id="fs-id2900205"><newline/> Then,
<newline/>
</para><equation id="fs-id2900208" class="unnumbered"><m:math display="block">
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mtable columnalign="left">
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>tan</m:mi><m:mo stretchy="false">(</m:mo><m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mo>+</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi></m:mrow>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mtext> </m:mtext><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>                </m:mtext><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mfrac>
           <m:mn>3</m:mn>
           <m:mn>4</m:mn>
          </m:mfrac>
          <m:mo>+</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>12</m:mn></m:mrow>
           <m:mn>5</m:mn>
          </m:mfrac>
          </m:mrow>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>−</m:mo><m:mfrac>
           <m:mn>3</m:mn>
           <m:mn>4</m:mn>
          </m:mfrac>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mfrac>
             <m:mrow>
              <m:mn>12</m:mn></m:mrow>
             <m:mn>5</m:mn>
            </m:mfrac>
            </m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>                </m:mtext><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mtext>  </m:mtext><m:mfrac>
           <m:mrow>
            <m:mn>63</m:mn></m:mrow>
           <m:mrow>
            <m:mn>20</m:mn></m:mrow>
          </m:mfrac>
          </m:mrow>
         <m:mrow>
          <m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>16</m:mn></m:mrow>
           <m:mrow>
            <m:mn>20</m:mn></m:mrow>
          </m:mfrac>
          </m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>​</m:mtext><m:mtext>                </m:mtext><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>63</m:mn></m:mrow>
         <m:mrow>
          <m:mn>16</m:mn></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
 </m:mtable>
 
</m:math>

</equation></item>
          <item>To find<m:math>
            <m:mrow>
              <m:mtext> </m:mtext><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>α</m:mi><m:mo>−</m:mo><m:mi>β</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
            </m:mrow>
          </m:math> we have the values we need. We can substitute them in and evaluate.
            <equation id="fs-id2575375" class="unnumbered"><m:math display="block">
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mtable columnalign="left">
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mi>α</m:mi><m:mo>−</m:mo><m:mi>β</m:mi></m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mo>−</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi></m:mrow>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>α</m:mi><m:mtext> </m:mtext><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>β</m:mi></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>                </m:mtext><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mfrac>
           <m:mn>3</m:mn>
           <m:mn>4</m:mn>
          </m:mfrac>
          <m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>12</m:mn></m:mrow>
           <m:mn>5</m:mn>
          </m:mfrac>
          </m:mrow>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>+</m:mo><m:mfrac>
           <m:mn>3</m:mn>
           <m:mn>4</m:mn>
          </m:mfrac>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mfrac>
             <m:mrow>
              <m:mn>12</m:mn></m:mrow>
             <m:mn>5</m:mn>
            </m:mfrac>
            </m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>                </m:mtext><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>33</m:mn></m:mrow>
           <m:mrow>
            <m:mn>20</m:mn></m:mrow>
          </m:mfrac>
          </m:mrow>
         <m:mrow>
          <m:mfrac>
           <m:mrow>
            <m:mn>56</m:mn></m:mrow>
           <m:mrow>
            <m:mn>20</m:mn></m:mrow>
          </m:mfrac>
          </m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mtext>                </m:mtext><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>33</m:mn></m:mrow>
         <m:mrow>
          <m:mn>56</m:mn></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mrow/>
