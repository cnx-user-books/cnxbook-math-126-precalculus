      <m:mn>15</m:mn></m:mrow>
    </m:msqrt>
    <m:mtext> </m:mtext><m:mtext>(x positive in QIV)</m:mtext>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mrow/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:msup>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
