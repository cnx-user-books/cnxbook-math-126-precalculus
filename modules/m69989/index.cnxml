   <m:mrow>
    <m:mn>13</m:mn></m:mrow>
  </m:mfrac>
  </m:mrow>
</m:math>, <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>π</m:mi></m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mo>≤</m:mo><m:mi>α</m:mi><m:mo>≤</m:mo><m:mn>2</m:mn><m:mi>π</m:mi></m:mrow>
</m:math>
, <m:math>
 <m:mrow>
  <m:mi>tan</m:mi><m:mi>β</m:mi><m:mo>=</m:mo><m:mfrac>
