          </m:mtd>
          <m:mtd>
           <m:mo>⇒</m:mo>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>C</m:mi></m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mo>⇒</m:mo>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>*</m:mo><m:mi>A</m:mi><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mo>&amp;</m:mo><m:mtext>​</m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>⋅</m:mo><m:mfrac>
             <m:mn>1</m:mn>
             <m:mi>B</m:mi>
            </m:mfrac>
            </m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mo>⇒</m:mo>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>+</m:mo><m:mi>D</m:mi></m:mrow>
          </m:mtd>
         </m:mtr>
         
        </m:mtable>
       </m:mtd>
      </m:mtr>
     </m:mtable>
     
    </m:mtd>
