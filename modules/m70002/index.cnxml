 <m:mo>,</m:mo><m:mtext>​</m:mtext><m:mtext>​</m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>π</m:mi><m:mo>,</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mfrac>
  <m:mrow>
   <m:mn>3</m:mn><m:mi>π</m:mi></m:mrow>
  <m:mn>2</m:mn>
 </m:mfrac>
 <m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext>and</m:mtext><m:mtext> </m:mtext><m:mn>2</m:mn><m:mi>π</m:mi>
</m:math>
