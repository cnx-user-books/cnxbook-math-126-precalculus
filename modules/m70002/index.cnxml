      <m:mtr>
       <m:mtd>
        <m:mtext>We use C if we do the horizontal first!</m:mtext>
       </m:mtd>
      </m:mtr>
      <m:mtr>
       <m:mtd>
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr>
       <m:mtd>
        <m:mtable>
         <m:mtr>
          <m:mtd>
           <m:mrow>
            <m:mtext>base</m:mtext><m:mtext> </m:mtext><m:mtext>point (a, b)</m:mtext></m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mo>⇒</m:mo>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>*</m:mo><m:mi>A</m:mi><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mo>&amp;</m:mo><m:mtext>​</m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>⋅</m:mo><m:mfrac>
             <m:mn>1</m:mn>
             <m:mi>B</m:mi>
            </m:mfrac>
            </m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mo>⇒</m:mo>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi></m:mrow>
