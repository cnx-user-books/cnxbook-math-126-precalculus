     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>y</m:mi><m:mi>tan</m:mi><m:msup>
         <m:mrow>
          <m:mn>25</m:mn></m:mrow>
         <m:mo>∘</m:mo>
        </m:msup>
        <m:mo>=</m:mo><m:mn>500</m:mn></m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>500</m:mn></m:mrow>
         <m:mrow>
          <m:mi>tan</m:mi><m:msup>
           <m:mrow>
            <m:mn>25</m:mn></m:mrow>
           <m:mo>∘</m:mo>
          </m:msup>
          </m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
