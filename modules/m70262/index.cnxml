        <m:mn>65</m:mn></m:mrow>
      </m:mfrac>
      <m:mo>,</m:mo><m:mi>cos</m:mi><m:mo> </m:mo><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>56</m:mn></m:mrow>
       <m:mrow>
        <m:mn>65</m:mn></m:mrow>
      </m:mfrac>
      <m:mo>,</m:mo><m:mi>t</m:mi><m:mi>a</m:mi><m:mi>n</m:mi><m:mo> </m:mo><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>33</m:mn></m:mrow>
       <m:mrow>
