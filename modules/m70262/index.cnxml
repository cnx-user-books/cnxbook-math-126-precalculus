          <m:mi>tan</m:mi><m:msup>
           <m:mrow>
            <m:mn>31</m:mn></m:mrow>
           <m:mo>∘</m:mo>
          </m:msup>
          </m:mrow>
        </m:mfrac>
        <m:mo>+</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>500</m:mn></m:mrow>
         <m:mrow>
          <m:mi>tan</m:mi><m:msup>
           <m:mrow>
            <m:mn>25</m:mn></m:mrow>
           <m:mo>∘</m:mo>
          </m:msup>
          </m:mrow>
        </m:mfrac>
        <m:mo>≈</m:mo><m:mn>1904.3932</m:mn><m:mtext> </m:mtext><m:mi>f</m:mi><m:mi>t</m:mi></m:mrow>
      </m:mtd>
     </m:mtr>
     
    </m:mtable>
