        <m:mtr>
         <m:mtd>
          <m:mtext>Now</m:mtext><m:mtext> </m:mtext><m:mtext>put</m:mtext><m:mtext> </m:mtext><m:mtext>it</m:mtext><m:mtext> </m:mtext><m:mtext>together:</m:mtext>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mrow/>
         </m:mtd>
        </m:mtr>
       </m:mtable>
       
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>d</m:mi><m:mo>=</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>500</m:mn></m:mrow>
