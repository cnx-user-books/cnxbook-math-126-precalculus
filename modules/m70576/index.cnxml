         <m:mrow>
          <m:mn>5</m:mn><m:msqrt>
           <m:mrow>
            <m:mn>29</m:mn></m:mrow>
          </m:msqrt>
          </m:mrow>
         <m:mrow>
          <m:mn>29</m:mn></m:mrow>
        </m:mfrac>
        </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="left">
      <m:mtd columnalign="left">
       <m:mrow>
        <m:mi>tan</m:mi><m:mo stretchy="false">(</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mtext>Opposite</m:mtext></m:mrow>
         <m:mrow>
          <m:mtext>Adjacent</m:mtext></m:mrow>
        </m:mfrac>
        <m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>2</m:mn></m:mrow>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>5</m:mn></m:mrow>
        </m:mfrac>
        <m:mo>=</m:mo><m:mfrac>
         <m:mn>2</m:mn>
