      </m:mfrac>
      <m:mo>=</m:mo><m:mi>x</m:mi><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>sec</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mi>h</m:mi><m:mi>y</m:mi><m:mi>p</m:mi><m:mi>o</m:mi><m:mi>t</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>u</m:mi><m:mi>s</m:mi><m:mi>e</m:mi></m:mrow>
       <m:mrow>
        <m:mi>a</m:mi><m:mi>d</m:mi><m:mi>j</m:mi><m:mi>a</m:mi><m:mi>c</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>t</m:mi></m:mrow>
