 <problem id="fs-id1165137897088">
  <para id="fs-id1165137897090"><m:math>
 <m:mo>−</m:mo><m:mfrac>
  <m:mrow>
   <m:mn>5</m:mn><m:mi>π</m:mi></m:mrow>
  <m:mn>6</m:mn>
 </m:mfrac>
 
</m:math>

  </para></problem> 
<solution id="fs-id1165135192734">
 <para id="fs-id1165135192736"><m:math>
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mi>sin</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
     <m:mrow>
      <m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      </m:mrow>
     <m:mn>2</m:mn>
    </m:mfrac>
    <m:mo>;</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>cos</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
     <m:mn>1</m:mn>
     <m:mn>2</m:mn>
    </m:mfrac>
    <m:mtext> </m:mtext><m:mo>;</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>tan</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:msqrt>
     <m:mn>3</m:mn>
    </m:msqrt>
    
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>csc</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>2</m:mn><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      </m:mrow>
     <m:mn>3</m:mn>
    </m:mfrac>
    <m:mo>;</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>sec</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>;</m:mo><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>cot</m:mi><m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>3</m:mn><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      </m:mrow>
     <m:mn>3</m:mn>
    </m:mfrac>
    
   </m:mtd>
  </m:mtr>
 </m:mtable>
 
</m:math>
</para></solution>
</exercise>

