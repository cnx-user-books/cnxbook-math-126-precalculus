  </m:math>
  </para></problem>
 </exercise><para id="fs-id1663752">For the following exercises, write the partial fraction decomposition.</para>
  <exercise id="fs-id1663755"><problem id="fs-id1663756"><para id="fs-id1663757"><m:math>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>30</m:mn>
     </m:mrow>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn>
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  </m:math>
  </para></problem></exercise>
  <exercise id="fs-id1507242"><problem id="fs-id1507243"><para id="fs-id1598618"><m:math>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>13</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
     </m:mrow>
     <m:mrow>
      <m:msup>
       <m:mrow>
        <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  </m:math>
  </para></problem>
  <solution id="fs-id1519236">
   <para id="fs-id1519238"><m:math>
    <m:mrow>
     <m:mfrac>
      <m:mn>5</m:mn>
      <m:mrow>
       <m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:mfrac>
     <m:mo>−</m:mo><m:mfrac>
      <m:mrow>
       <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
      </m:mrow>
      <m:mrow>
       <m:msup>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mn>2</m:mn>
       </m:msup>
       
      </m:mrow>
     </m:mfrac>
     
    </m:mrow>
   </m:math>
   </para>
</solution></exercise>
  <exercise id="fs-id1698396"><problem id="fs-id1698397"><para id="fs-id1698398"><m:math>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>4</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>3</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mi>x</m:mi><m:msup>
       <m:mrow>
        <m:mo stretchy="false">(</m:mo><m:msup>
         <m:mi>x</m:mi>
         <m:mn>2</m:mn>
        </m:msup>
        <m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  </m:math>
  </para></problem></exercise>
  
