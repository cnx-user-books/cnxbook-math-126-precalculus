  </m:math>
  </para></problem></exercise>
  <exercise id="fs-id1536358"><problem id="fs-id1536359"><para id="fs-id1536360"><m:math>
   <m:mtable columnalign="left">
    <m:mtr>
     <m:mtd>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>&gt;</m:mo><m:mn>4</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>y</m:mi><m:mo>&lt;</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
