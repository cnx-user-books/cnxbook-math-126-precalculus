  <exercise id="fs-id1780639"><problem id="fs-id1780640"><para id="fs-id1780641"><m:math>
 <m:mrow>
  <m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>4</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mn>10</m:mn>
       </m:mrow>
      </m:mfrac>
      
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>7</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>4</m:mn>
      </m:mfrac>
      
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>9</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mn>6</m:mn>
       <m:mn>5</m:mn>
      </m:mfrac>
      
     </m:mrow>
    </m:mtd>
   </m:mtr>
   
  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
   <solution id="fs-id1455837">
    <para id="fs-id1455840"><m:math>
     <m:mrow>
      <m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mn>20</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>10</m:mn><m:mo>,</m:mo><m:mn>5</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para>
   </solution></exercise>
  <exercise id="fs-id1675200"><problem id="fs-id1675201"><para id="fs-id1675202"><m:math>
   <m:mtable columnalign="right">
    <m:mtr>
     <m:mtd>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>20</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>20</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>10</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>
   
  </m:math>
  </para></problem></exercise>
  <exercise id="fs-id1436662"><problem id="fs-id1436663"><para id="fs-id1436664"><m:math>
   <m:mtable columnalign="right">
    <m:mtr>
     <m:mtd>
      <m:mn>5</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>7</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>
   
  </m:math>
  </para></problem>
   <solution id="fs-id1615808">
    <para id="fs-id1615810"><m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mi>x</m:mi><m:mo>,</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>16</m:mn><m:mi>x</m:mi>
         </m:mrow>
         <m:mn>5</m:mn>
        </m:mfrac>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>13</m:mn><m:mi>x</m:mi>
         </m:mrow>
         <m:mn>5</m:mn>
        </m:mfrac>
        
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para>
   </solution></exercise>
  <exercise id="fs-id1537027"><problem id="fs-id1537028"><para id="fs-id1537029"><m:math>
   <m:mtable columnalign="left">
    <m:mtr>
     <m:mtd>
      <m:mi>y</m:mi><m:mo>=</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>y</m:mi><m:mo>=</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>
   
  </m:math>
  </para></problem></exercise>
  <exercise id="fs-id1432229"><problem id="fs-id1432230"><para id="fs-id1432231"><m:math>
   <m:mtable columnalign="left">
    <m:mtr>
     <m:mtd>
      <m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>25</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>
   
  </m:math>
  </para></problem>
   <solution id="fs-id1703246">
    <para id="fs-id1703248"><m:math>
     <m:mrow>
      <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:msqrt>
       <m:mn>2</m:mn>
      </m:msqrt>
      <m:mo>,</m:mo><m:mo>−</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>17</m:mn>
       </m:mrow>
      </m:msqrt>
      <m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>2</m:mn><m:msqrt>
         <m:mn>2</m:mn>
        </m:msqrt>
        <m:mo>,</m:mo><m:msqrt>
         <m:mrow>
          <m:mn>17</m:mn>
         </m:mrow>
        </m:msqrt>
        
       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>2</m:mn><m:msqrt>
          <m:mn>2</m:mn>
         </m:msqrt>
         <m:mo>,</m:mo><m:mo>−</m:mo><m:msqrt>
          <m:mrow>
           <m:mn>17</m:mn>
          </m:mrow>
         </m:msqrt>
         
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>2</m:mn><m:msqrt>
           <m:mn>2</m:mn>
          </m:msqrt>
          <m:mo>,</m:mo><m:msqrt>
           <m:mrow>
            <m:mn>17</m:mn>
           </m:mrow>
          </m:msqrt>
          
         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para>
</solution></exercise>
  
  <para id="fs-id1384560">For the following exercises, graph the following inequalities.</para>
  <exercise id="fs-id1384563"><problem id="fs-id1384564"><para id="fs-id1384565"><m:math>
