<solution id="fs-id1165134134227"><para id="fs-id1165135210063"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mi>x</m:mi>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
