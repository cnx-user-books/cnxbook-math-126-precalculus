 <m:mrow>
  <m:mtext> </m:mtext><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>cos</m:mi><m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:mo>,</m:mo>
 </m:mrow>
</m:math> where<m:math>
