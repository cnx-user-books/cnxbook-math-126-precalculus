  </m:mtable>
 </m:mrow>
</m:math>
</equation></solution>
     </exercise></example>
<note id="eip-id1165133258181" class="precalculus try">
<label>Try It</label>
<exercise id="ti_05_03_09">
     <problem id="fs-id1165137872027"><para id="fs-id1165137872028">If<m:math>
      <m:mrow>
       <m:mtext> </m:mtext><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:msqrt>
           <m:mn>2</m:mn>
          </m:msqrt>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:mfrac>
       <m:mtext> </m:mtext></m:mrow>
     </m:math>
      and<m:math>
       <m:mrow>
        <m:mtext> </m:mtext><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
