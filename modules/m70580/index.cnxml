       <m:mn>1</m:mn>
       <m:mrow>
        <m:mi>sin</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi>
       </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>
          
         </m:mrow>
