     </m:math> and<m:math>
      <m:mrow>
       <m:mtext> </m:mtext><m:mn>0</m:mn><m:mo>≤</m:mo><m:mi>t</m:mi><m:mo>&lt;</m:mo><m:mfrac>
        <m:mi>π</m:mi>
        <m:mn>2</m:mn>
       </m:mfrac>
       <m:mo>,</m:mo></m:mrow>
     </m:math> find<m:math>
 <m:mrow>
  <m:mtext> </m:mtext><m:mi>sin</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>cos</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>sec</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>csc</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo>
 </m:mrow>
</m:math> and <m:math>
 <m:mrow>
  <m:mtext> </m:mtext><m:mi>cot</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>.</m:mo>
 </m:mrow>
</m:math>
     </para></problem>
   </exercise>


   <exercise id="fs-id1165137827134" type="Chapter5S1-4">
    <problem id="fs-id1165137827136">
     <para id="fs-id1165135153096">If<m:math>
      <m:mrow>
       <m:mtext> </m:mtext><m:mi>sin</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
        <m:mrow>
         <m:msqrt>
          <m:mn>3</m:mn>
         </m:msqrt>
        </m:mrow>
        <m:mn>2</m:mn>
       </m:mfrac>
       <m:mtext> </m:mtext></m:mrow>
     </m:math>and<m:math>
      <m:mrow>
       <m:mtext> </m:mtext><m:mi>cos</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
        <m:mn>1</m:mn>
        <m:mn>2</m:mn>
       </m:mfrac>
       <m:mo>,</m:mo></m:mrow>
     </m:math> find<m:math>
 <m:mrow>
  <m:mtext> </m:mtext><m:mi>sec</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>csc</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>,</m:mo>
 </m:mrow>
</m:math> and <m:math>
      <m:mrow>
       <m:mtext> </m:mtext><m:mi>cot</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>.</m:mo></m:mrow>
     </m:math>
     </para></problem>
   <solution id="fs-id1165135415735"> 
    <para id="fs-id1165135199609"><m:math>
     <m:mrow>
      <m:mi>sec</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mi>csc</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>2</m:mn><m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
       </m:mrow>
       <m:mn>3</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      <m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>cot</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
       </m:mrow>
       <m:mn>3</m:mn>
      </m:mfrac>
     </m:mrow>
    </m:math>
    </para></solution>
   </exercise>


 <exercise id="fs-id1165137883770" type="Chapter5S1-4">
