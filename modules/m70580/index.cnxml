       </m:msqrt>
      </m:mrow>
      <m:mn>3</m:mn>
     </m:mfrac>
     <m:mo>,</m:mo><m:mi>sec</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mi>csc</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
      <m:mrow>
