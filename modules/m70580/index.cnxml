     <solution id="fs-id1165134148472"><para id="fs-id1165134148474"><m:math>
      <m:mrow>
       <m:mi>sec</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
        <m:mn>2</m:mn>
       </m:msqrt>
       <m:mo>,</m:mo><m:mi>csc</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
        <m:mn>2</m:mn>
       </m:msqrt>
       <m:mo>,</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mi>cot</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow>
     </m:math>
     </para></solution>
    </exercise>
</note></section></section>
 
 
