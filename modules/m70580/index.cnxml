   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>Q</m:mi><m:mi>I</m:mi><m:mi>I</m:mi><m:mi>I</m:mi><m:mo>:</m:mo><m:mtext>​</m:mtext><m:mfrac>
