     <m:mo>,</m:mo><m:mi>tan</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
      <m:mn>3</m:mn>
     </m:msqrt>
     <m:mo>,</m:mo><m:mi>cot</m:mi><m:mtext> </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
      <m:mrow>
