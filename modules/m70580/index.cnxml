 </m:semantics>
</m:math>
  </para>
</problem>

<solution id="eip-idm1208681840">
  <para id="eip-idm1208681584"><m:math>
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:mi>Q</m:mi><m:mi>I</m:mi><m:mo>:</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>2</m:mn><m:mtext> </m:mtext><m:mi>π</m:mi></m:mrow>
     <m:mn>9</m:mn>
    </m:mfrac>
    
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>Q</m:mi><m:mi>I</m:mi><m:mi>I</m:mi><m:mo>:</m:mo><m:mtext>​</m:mtext><m:mfrac>
